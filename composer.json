{
    "name": "israelwebdev/plaid-sdk-php",
    "description": "PHP implementation of Plaid's API as an SDK (Income enhanced).",
    "keywords": [
        "plaid",
        "sdk",
        "php"
    ],
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-curl": "*",
        "nimbly/shuttle": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "TomorrowIdeas\\Plaid\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "vimeo/psalm": "^3.3",
        "symfony/var-dumper": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "TomorrowIdeas\\Plaid\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@tomorrow.me"
        },
        {
            "name": "Zvi Landsman",
            "email": "zvi@israelwebdev.com"
        }
    ]
}
